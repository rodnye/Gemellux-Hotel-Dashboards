          {
            "csvContent": "Tipo,Valor\nReal,4.3\nPlan,2.4\nReal AA,2",
            "datasource": {
              "type": "grafana-testdata-datasource",
              "uid": "bf4278l56vabkf"
            },
            "dropPercent": 0,
            "editorMode": "code",
            "format": "table",
            "rawQuery": true,
            "rawSql": "WITH vars AS (\n  SELECT\n    '$carrier_type' AS carrierType,\n    to_timestamp($__from / 1000)::date AS date_from,\n    to_timestamp($__to   / 1000)::date AS date_to\n),\nparsed AS (\n  SELECT\n    to_timestamp((o.recvtimets)::bigint / 1000) AS recvtimets,\n    o.dateobserved::date AS dateobserved,\n    c AS carrier\n  FROM gemelluxiot.x002fconsumptionobserved o\n  CROSS JOIN LATERAL jsonb_array_elements(o.carriers::jsonb) AS c\n  CROSS JOIN vars v\n  WHERE c -\u003e\u003e 'carrierType' = v.carrierType\n    AND o.dateobserved::date BETWEEN v.date_from AND v.date_to\n),\nvalues_raw AS (\n  SELECT\n    (carrier -\u003e\u003e 'coverage')::int AS coverage,\n    '(' || (carrier -\u003e\u003e 'unit') || ')' AS unit,\n    recvtimets,\n\n    to_char(recvtimets, 'DD/MM/YYYY') AS formated_date,\n\n    -- valores anuales\n    (carrier -\u003e\u003e 'yearPlan')::numeric           AS plan_year,\n    (carrier -\u003e\u003e 'yearReal')::numeric           AS real_year,\n    (carrier -\u003e\u003e 'yearRealLastYear')::numeric   AS real_aa_year,\n\n    -- valores mensuales\n    (carrier -\u003e\u003e 'monthPlan')::numeric          AS plan_month,\n    (carrier -\u003e\u003e 'monthReal')::numeric          AS real_month,\n    (carrier -\u003e\u003e 'monthRealLastYear')::numeric  AS real_aa_month,\n\n    -- valores diarios\n    (carrier -\u003e\u003e 'dayPlan')::numeric            AS plan_day,\n    (carrier -\u003e\u003e 'dayReal')::numeric            AS real_day,\n    (carrier -\u003e\u003e 'dayRealLastYear')::numeric    AS real_aa_day\n  FROM parsed\n  ORDER BY recvtimets\n  LIMIT 1\n),\ncalculated_values AS (\n  SELECT\n    *,\n    CASE \n      WHEN real_year \u003e 0 THEN\n        ROUND(((real_aa_year - real_year) / real_year) * 100, 1)\n      ELSE 0\n    END AS percent_aa_year,\n    CASE \n      WHEN real_year \u003e 0 THEN\n        ROUND(((plan_year - real_year) / real_year) * 100, 1)\n      ELSE 0\n    END AS percent_plan_year,\n    \n    CASE \n      WHEN real_month \u003e 0 THEN\n        ROUND(((real_aa_month - real_month) / real_month) * 100, 1)\n      ELSE 0\n    END AS percent_aa_month,\n    CASE \n      WHEN real_month \u003e 0 THEN\n        ROUND(((plan_month - real_month) / real_month) * 100, 1)\n      ELSE 0\n    END AS percent_plan_month,\n    \n    -- Cálculos porcentuales para valores diarios\n    CASE \n      WHEN real_day \u003e 0 THEN\n        ROUND(((real_aa_day - real_day) / real_day) * 100, 1)\n      ELSE 0\n    END AS percent_aa_day,\n    CASE \n      WHEN real_day \u003e 0 THEN\n        ROUND(((plan_day - real_day) / real_day) * 100, 1)\n      ELSE 0\n    END AS percent_plan_day\n  FROM values_raw\n)\nSELECT\n  'Anual ' AS categoria,\n  CASE \n    WHEN percent_aa_year \u003e 0 THEN '+' || percent_aa_year::text || '% ▲'\n    WHEN percent_aa_year \u003c 0 THEN percent_aa_year::text || '% ▼'\n    ELSE '0.0%'\n  END AS \"Porcentaje AA\",\n  real_aa_year AS \"Real AA\",\n  CASE \n    WHEN percent_plan_year \u003e 0 THEN '+' || percent_plan_year::text || '% ▲'\n    WHEN percent_plan_year \u003c 0 THEN percent_plan_year::text || '% ▼'\n    ELSE '0.0%'\n  END AS \"Porcentaje Plan\",\n  plan_year AS \"Plan\",\n  real_year AS \"Real\",\n  formated_date\nFROM calculated_values\n\nUNION ALL\nSELECT\n  'Mensual ' AS categoria,\n  CASE \n    WHEN percent_aa_month \u003e 0 THEN '+' || percent_aa_month::text || '% ▲'\n    WHEN percent_aa_month \u003c 0 THEN percent_aa_month::text || '% ▼'\n    ELSE '0.0%'\n  END AS \"Porcentaje AA\",\n  real_aa_month AS \"Real AA\",\n  CASE \n    WHEN percent_plan_month \u003e 0 THEN '+' || percent_plan_month::text || '% ▲'\n    WHEN percent_plan_month \u003c 0 THEN percent_plan_month::text || '% ▼'\n    ELSE '0.0%'\n  END AS \"Porcentaje Plan\",\n  plan_month AS \"Plan\",\n  real_month AS \"Real\",\n  formated_date\nFROM calculated_values\n\nUNION ALL\nSELECT\n  'Diario ' AS categoria,\n  CASE \n    WHEN percent_aa_day \u003e 0 THEN '+' || percent_aa_day::text || '% ▲'\n    WHEN percent_aa_day \u003c 0 THEN percent_aa_day::text || '% ▼'\n    ELSE '0.0%'\n  END AS \"Porcentaje AA\",\n  real_aa_day AS \"Real AA\",\n  CASE \n    WHEN percent_plan_day \u003e 0 THEN '+' || percent_plan_day::text || '% ▲'\n    WHEN percent_plan_day \u003c 0 THEN percent_plan_day::text || '% ▼'\n    ELSE '0.0%'\n  END AS \"Porcentaje Plan\",\n  plan_day AS \"Plan\",\n  real_day AS \"Real\",\n  formated_date\nFROM calculated_values;",
