        "targets": [
          {
            "csvContent": "Tipo,Valor\nReal,4.3\nPlan,2.4\nReal AA,2",
            "datasource": {
              "type": "grafana-testdata-datasource",
              "uid": "bf4278l56vabkf"
            },
            "dropPercent": 0,
            "editorMode": "code",
            "format": "table",
            "rawQuery": true,
            "rawSql": "WITH vars AS (\n  SELECT\n    '$carrier_type' AS carrierType,\n    '$period' AS period,\n    to_timestamp($__from / 1000) :: date AS date_from,\n    to_timestamp($__to / 1000) :: date AS date_to\n),\nparsed AS (\n  SELECT\n    to_timestamp((o.recvtimets) :: bigint / 1000) AS recvtimets,\n    o.dateobserved :: date AS dateobserved,\n    c AS carrier\n  FROM\n    gemelluxiot.x002fconsumptionobserved o\n    CROSS JOIN LATERAL jsonb_array_elements(o.carriers :: jsonb) AS c\n    CROSS JOIN vars v\n  WHERE\n    c -\u003e\u003e 'carrierType' = v.carrierType\n    AND o.dateobserved :: date BETWEEN v.date_from\n    AND v.date_to\n),\nvalues_raw AS (\n  SELECT\n    (carrier -\u003e\u003e 'coverage') :: int AS coverage,\n    '(' || (carrier -\u003e\u003e 'unit') || ')' AS unit,\n    v.period AS period,\n    CASE v.period\n        WHEN 'day'   THEN to_char(recvtimets, 'DD/MM/YYYY')\n        WHEN 'month' THEN to_char(recvtimets, 'TMMonth YYYY')\n        WHEN 'year'  THEN to_char(recvtimets, 'YYYY')\n    END AS formated_date,\n    CASE\n      v.period\n      WHEN 'year' THEN (carrier -\u003e\u003e 'yearPlan') :: numeric\n      WHEN 'month' THEN (carrier -\u003e\u003e 'monthPlan') :: numeric\n      WHEN 'day' THEN (carrier -\u003e\u003e 'dayPlan') :: numeric\n    END AS plan_value,\n    CASE\n      v.period\n      WHEN 'year' THEN (carrier -\u003e\u003e 'yearReal') :: numeric\n      WHEN 'month' THEN (carrier -\u003e\u003e 'monthReal') :: numeric\n      WHEN 'day' THEN (carrier -\u003e\u003e 'dayReal') :: numeric\n    END AS real_value,\n    CASE\n      v.period\n      WHEN 'year' THEN (carrier -\u003e\u003e 'yearRealLastYear') :: numeric\n      WHEN 'month' THEN (carrier -\u003e\u003e 'monthRealLastYear') :: numeric\n      WHEN 'day' THEN (carrier -\u003e\u003e 'dayRealLastYear') :: numeric\n    END AS real_last_year\n  FROM\n    parsed\n    CROSS JOIN vars v\n  ORDER BY\n    recvtimets\n  LIMIT\n    1\n),\ncalculated_values AS (\n  SELECT\n    *,\n    CASE \n      WHEN real_value \u003e 0 THEN\n        ROUND(((plan_value - real_value) / real_value) * 100, 1)\n      ELSE 0\n    END AS percent_vs_real_plan,\n    CASE \n      WHEN real_value \u003e 0 THEN\n        ROUND(((real_last_year - real_value) / real_value) * 100, 1)\n      ELSE 0\n    END AS percent_vs_real_last_year,\n\n    CASE \n      WHEN plan_value \u003e real_value THEN\n        CASE \n          WHEN ABS(plan_value - real_value) / NULLIF(plan_value, 0) \u003c 0.05 \n            THEN 'Consumo ligeramente inferior al plan'\n          WHEN ABS(plan_value - real_value) / NULLIF(plan_value, 0) \u003c 0.15 \n            THEN 'Consumo moderadamente inferior al plan'\n          ELSE 'Consumo significativamente inferior al plan'\n        END\n      WHEN plan_value \u003c real_value THEN\n        CASE \n          WHEN ABS(real_value - plan_value) / NULLIF(plan_value, 0) \u003c 0.05 \n            THEN 'Consumo ligeramente superior al plan'\n          WHEN ABS(real_value - plan_value) / NULLIF(plan_value, 0) \u003c 0.15 \n            THEN 'Consumo moderadamente superior al plan'\n          ELSE 'Consumo significativamente superior al plan'\n        END\n      ELSE 'Consumo alineado con el plan'\n    END AS vs_plan_message,\n\n    CASE \n      WHEN real_last_year \u003e real_value THEN\n        CASE \n          WHEN ABS(real_last_year - real_value) / NULLIF(real_last_year, 0) \u003c 0.05 \n            THEN 'Consumo ligeramente inferior al año pasado'\n          WHEN ABS(real_last_year - real_value) / NULLIF(real_last_year, 0) \u003c 0.15 \n            THEN 'Consumo moderadamente inferior al año pasado'\n          ELSE 'Consumo significativamente inferior al año pasado'\n        END\n      WHEN real_last_year \u003c real_value THEN\n        CASE \n          WHEN ABS(real_value - real_last_year) / NULLIF(real_last_year, 0) \u003c 0.05 \n            THEN 'Consumo ligeramente superior al año pasado'\n          WHEN ABS(real_value - real_last_year) / NULLIF(real_last_year, 0) \u003c 0.15 \n            THEN 'Consumo moderadamente superior al año pasado'\n          ELSE 'Consumo significativamente superior al año pasado'\n        END\n      ELSE 'Consumo similar al del año pasado'\n    END AS vs_last_year_message,\n    -- Versiones cortas para tooltips o espacios reducidos\n    CASE \n      WHEN plan_value \u003e real_value THEN 'Por debajo del plan'\n      WHEN plan_value \u003c real_value THEN 'Por encima del plan'\n      ELSE 'Alineado con plan'\n    END AS vs_plan_short,\n    CASE \n      WHEN real_last_year \u003e real_value THEN 'Por debajo del año anterior'\n      WHEN real_last_year \u003c real_value THEN 'Por encima del año anterior'\n      ELSE 'Similar al año anterior'\n    END AS vs_last_year_short\n  FROM\n    values_raw\n)\nSELECT\n  'Real ' || unit AS type,\n  real_value AS value,\n  formated_date,\n  '' AS percent_change,\n  0 AS percent,\n  '' AS comparison_message,\n  'Consumo actual' AS comparison_status\nFROM\n  calculated_values\nUNION ALL\nSELECT\n  'Plan ' || unit,\n  plan_value AS value,\n  formated_date,\n  CASE \n    WHEN percent_vs_real_plan \u003e 0 THEN '+' || percent_vs_real_plan::text || '% ▲'\n    WHEN percent_vs_real_plan \u003c 0 THEN '-' || (percent_vs_real_plan * -1)::text || '% ▼'\n    ELSE '0.0%'\n  END AS percent_change,\n  percent_vs_real_plan AS percent,\n  vs_plan_message AS comparison_message,\n  vs_plan_short AS comparison_status\nFROM\n  calculated_values\nUNION ALL\nSELECT\n  'Real AA ' || unit,\n  real_last_year AS value,\n  formated_date,\n  CASE \n    WHEN percent_vs_real_last_year \u003e 0 THEN '+' || percent_vs_real_last_year::text || '% ▲'\n    WHEN percent_vs_real_last_year \u003c 0 THEN '-' || (percent_vs_real_last_year * -1)::text || '% ▼'\n    ELSE '0.0%'\n  END AS percent_change,\n  percent_vs_real_last_year AS percent,\n  vs_last_year_message AS comparison_message,\n  vs_last_year_short AS comparison_status\nFROM\n  calculated_values",
            "refId": "A",
