          {
            "csvContent": "Tipo,Valor\nReal,4.3\nPlan,2.4\nReal AA,2",
            "datasource": {
              "type": "grafana-testdata-datasource",
              "uid": "bf4278l56vabkf"
            },
            "dropPercent": 0,
            "editorMode": "code",
            "format": "table",
            "rawQuery": true,
            "rawSql": "WITH vars AS (\n  SELECT\n    '$carrier_type' AS carrierType,\n    '$period' AS period,\n    to_timestamp($__from / 1000) :: date AS date_from,\n    to_timestamp($__to / 1000) :: date AS date_to\n),\nparsed AS (\n  SELECT\n    to_timestamp((o.recvtimets) :: bigint / 1000) AS recvtimets,\n    o.dateobserved :: date AS dateobserved,\n    c AS carrier\n  FROM\n    gemelluxiot.x002fconsumptionobserved o\n    CROSS JOIN LATERAL jsonb_array_elements(o.carriers :: jsonb) AS c\n    CROSS JOIN vars v\n  WHERE\n    c -\u003e\u003e 'carrierType' = v.carrierType\n    AND o.dateobserved :: date BETWEEN v.date_from\n    AND v.date_to\n),\nvalues_raw AS (\n  SELECT\n    (carrier -\u003e\u003e 'coverage') :: int AS coverage,\n    '(' || (carrier -\u003e\u003e 'unit') || ')' AS unit,\n    v.period AS period,\n    CASE v.period\n        WHEN 'day'   THEN to_char(recvtimets, 'DD/MM/YYYY')\n        WHEN 'month' THEN to_char(recvtimets, 'TMMonth YYYY')\n        WHEN 'year'  THEN to_char(recvtimets, 'YYYY')\n    END AS formated_date,\n    CASE\n      v.period\n      WHEN 'year' THEN (carrier -\u003e\u003e 'yearPlan') :: numeric\n      WHEN 'month' THEN (carrier -\u003e\u003e 'monthPlan') :: numeric\n      WHEN 'day' THEN (carrier -\u003e\u003e 'dayPlan') :: numeric\n    END AS plan_value,\n    CASE\n      v.period\n      WHEN 'year' THEN (carrier -\u003e\u003e 'yearReal') :: numeric\n      WHEN 'month' THEN (carrier -\u003e\u003e 'monthReal') :: numeric\n      WHEN 'day' THEN (carrier -\u003e\u003e 'dayReal') :: numeric\n    END AS real_value,\n    CASE\n      v.period\n      WHEN 'year' THEN (carrier -\u003e\u003e 'yearRealLastYear') :: numeric\n      WHEN 'month' THEN (carrier -\u003e\u003e 'monthRealLastYear') :: numeric\n      WHEN 'day' THEN (carrier -\u003e\u003e 'dayRealLastYear') :: numeric\n    END AS real_last_year\n  FROM\n    parsed\n    CROSS JOIN vars v\n  ORDER BY\n    recvtimets\n  LIMIT\n    1\n)\nSELECT\n  'Real ' || unit AS type,\n  real_value AS value,\n  formated_date\nFROM\n  values_raw\nUNION\nALL\nSELECT\n  'Plan ' || unit,\n  plan_value AS value,\n  formated_date\nFROM\n  values_raw\nUNION\nALL\nSELECT\n  'Real AA ' || unit,\n  real_last_year AS value,\n  formated_date \nFROM\n  values_raw\n",
            "refId": "A",
